package javaBasics;

import java.util.Scanner;

public class FindNumbersFromUser {

	public static void main(String[] args) {
		int a=0;
		int i=0;
		
		Scanner sc =new Scanner(System.in);
		System.out.println("Enter 20 Numbers:");
		a=sc.nextInt();
		if(i>=a) {
			System.out.println(a);
		}
		
		

	}

}
