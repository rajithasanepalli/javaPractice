package javaBasics;

import java.util.Scanner;

public class FindNumbersFromUser {

	public static void main(String[] args) {
		int n;
		
		Scanner sc =new Scanner(System.in);
	
		n=sc.nextInt();
		int arr[]=new int[10];
		System.out.println("Enter Array Elements");
		
		for(int i=0;i<n;i++) {
			arr[i]=sc.nextInt();
		}
			
		
			
		}
	
		
		

	}


