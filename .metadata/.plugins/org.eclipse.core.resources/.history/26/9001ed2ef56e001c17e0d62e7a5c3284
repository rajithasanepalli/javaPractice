package accesmodifiers;

import java.sql.Date;

public class Report extends CEO {
	public int time;
	public String task;
	private Date date;
	
	
	Report(){}
	Report(int time,String task, Date date){
		time=10;
		this.time=time;
		this.task=task;
		this.date=date;
		
		
	}
	public Report(int i, String string, String string2) {
		// TODO Auto-generated constructor stub
	}
	public void viewReportDetals() {
		
		System.out.println("Reporting time="+time);
		System.out.println("Reporting task="+task);
		System.out.println("Reporting date="+date);
		
	}
	public int getTime() {
		return time;
	}
	public void setTime(int time) {
		this.time = time;
	}
	public String getTask() {
		return task;
	}
	public void setTask(String task) {
		this.task = task;
	}
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}

}
