package javaBasics;

import java.util.Arrays;

public class SameElementsOrNot {

	public static void main(String[] args) {
		int a[]= {5,5,9,5};
		if(a[0]==a[1]&a[0]==a[2]&&a[0]==a[3]) {
			System.out.println("All Elements in array equal");
			
		}
		else {
			System.out.println("All Elements in array are not  equal");
		}

	}

}
