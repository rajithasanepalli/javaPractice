package javaBasics;

public class SplitArray {

	public static void main(String[] args) {
		int a[] = { 58, 24, 13, 15, 63, 9, 8, 81, 1, 78 };

		System.out.println("Intial Array");
		for (int i = 0; i <= 4; i++) {
			System.out.print("" + a[i] + ",");
		}
		System.out.println("First Half");
		for(int j=5;j<=9;j++) {
			System.out.println(a[j]);
		}
	}

}
