package accesmodifiers;

import java.sql.Date;

public class Report extends Project {
	public int time=12;
	public String task="oops";
	public Date date=new Date(24-10-2010);
	
	
	Report(){}
	Report(int time,String task, Date date){
		this.time=time;
		this.task=task;
		this.date=date;
		
		
	}
	public void viewReportDetals() {
		
		System.out.println("Reporting time="+time);
		System.out.println("Reporting task="+task);
		System.out.println("Reporting date="+date);
		
	}
	public int getTime() {
		return time;
	}
	public void setTime(int time) {
		this.time = time;
	}
	public String getTask() {
		return task;
	}
	public void setTask(String task) {
		this.task = task;
	}
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}

}
